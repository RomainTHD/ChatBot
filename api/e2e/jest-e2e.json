{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "moduleDirectories": [
        "../src",
        "../node_modules"
    ],
    "rootDir": ".",
    "setupFiles": [
        "./jest.setup.ts"
    ],
    "setupFilesAfterEnv": [
        "jest-extended"
    ],
    "testEnvironment": "node",
    "testRegex": ".http.e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    }
}
